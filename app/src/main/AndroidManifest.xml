<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androidswipelayout" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MyActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.androidswipelayout.ListViewExample" />
        <activity android:name="com.androidswipelayout.GridViewExample" />
        <activity android:name="com.androidswipelayout.RecyclerViewExample" />
        <activity android:name=".NestedExample" />
    </application>

</manifest>
